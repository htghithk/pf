---
import Social from "./Social.astro";
import { socials } from "../GLOBALS.astro";
---

<div class="flex flex-wrap gap-x-8">
    {
        socials.map((item) => (
                <Social
                    title={item.title}
                    link={item.link}
                    bold={item.bold ? true : false}
                />
        ))
    }
</div>
