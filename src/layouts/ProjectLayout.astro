---
import Title from "../components/Title.astro";
import BasicLayout from "./BasicLayout.astro";

const {frontmatter} = Astro.props
---

<BasicLayout title=`Project | ${frontmatter.title}`>
    <Title title={frontmatter.title} description=`${frontmatter.description} | ${frontmatter.year}`/>
    <article class="mt-8 prose prose-h2:font-serif prose-h2:text-2xl
        prose-a:text-blue-600 prose-a:visited:text-purple-800
        prose-img:rounded-xl">
        <slot/>
    </article>
</BasicLayout>